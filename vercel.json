{
  "builds": [
    {
      "src": "SVO/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "SVO/wsgi.py"
    }
  ],
  "buildCommand": "chmod +x build_files.sh && ./build_files.sh",
  "outputDirectory": ""
}